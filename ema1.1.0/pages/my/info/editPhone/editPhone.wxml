<view>
  <view class="cu-form-group">
    <input auto-focus maxlength="11" disabled="{{false}}" bindinput="changeStatus" placeholder="电话"></input>
  </view>
  <view style="margin-top: 10rpx; margin-right: 20rpx;">
    <text class="text-grey" style="float: right; font-size: 25rpx;">{{length}}</text>
  </view>
  <view class=" padding flex flex-direction margin ">
    <button class="cu-btn bg-green lg " disabled="{{disable}}" bindtap="editPhone">确定</button>
  </view>
</view>